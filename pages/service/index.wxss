/* pages/service/index.wxss */
.s1 {
      height: 70rpx;
      background-color: #ec2312;
      color: #fff;
      margin-bottom: 20rpx;
    } 
    .text_mid {
      margin-left: 200rpx;
    }
    .s1_text {
      float: left;
    }
    .s1 image {
      float: left;
    }
   .s1 image:nth-child(3) {
     float: right;
   }


   .s0 {
  background-color: #f6f6f6;
  height: 1100rpx;
}
.main {
  width: 95%;
  margin: 0 auto;
}
.space {
  height: 20rpx;
  background-color: #f6f6f6;
}
.s2 {
  text-align: center;
  background-color: #fff;
  border-radius: 10rpx;
}
.s2 text {
  display: block;
  padding: 30rpx;
}

.s3 {
  background-color: #fff;
  border-radius: 15rpx;
  margin-top: 20rpx;
  padding-bottom: 20rpx;
}
.s3 li {
  display: flex;
  margin-left: 30rpx;
  height: 80rpx;
  align-items: center;
  font-size: 14px;
  border-bottom: 1px solid #ccc;
  width: 95%;
}
.s3 li input {
  color: #ccc;
  margin-left: 50rpx;
  font-size: 13px;
}

.s4 {
  margin-top: 10rpx;
  background-color: #fff;
  font-size: 12px;
  border-radius: 10rpx;
}
.s4 p {
  display: inline-block;
  width: 8rpx;
	height: 31rpx;
	background-color: #6180f1;
	border-radius: 4rpx;
  margin-top: 10rpx;
  margin-left: 20rpx;
}
.s4 h4 {
  font-size: 15px;
  margin-left: 10rpx;
}
.s4 image {
  display: block;
  margin-top: 10px;
  margin-left: 50rpx;
  padding-bottom: 30rpx;
}

.s5 {
  background-color: #fff;
  border-radius: 15rpx;
  margin-top: 20rpx;
  height: 160rpx;
}
.s5 input {
  display: inline-block;
}
.s6 {
  position: fixed;
  bottom: 0rpx;
  background-color: #fff;
  height: 80rpx;
  width: 100%;
  line-height: 80rpx;
}
.s6 text {
  float: right;
  width: 193rpx;
	height: 60rpx;
  line-height: 60rpx;
  margin-top: 10rpx;
  margin-right: 20rpx;
  color: #fff;
	background-image: linear-gradient(0deg, 
		#ed2502 0%, 
		#f84033 100%), 
	linear-gradient(
		#fdfdfe, 
		#fdfdfe);
	background-blend-mode: normal, 
		normal;
	border-radius: 36rpx;
  text-align: center;
}

