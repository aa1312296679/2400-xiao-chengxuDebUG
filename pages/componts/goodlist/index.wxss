.s7 {
     width: 95%;
     margin: 20rpx auto;
     display: flex;
     box-shadow: 3px 3px 3px #f5f5f5;
     border-radius: 5px;
   }
   .content_text{
     font-size: 25rpx;
     color: #7c7a7a;
   }
   .s7_text {
     font-size: 15px;
     color: #7c7a7a;
   }
   .s7_text text{
     color: red;
     font-weight: 650;
   }
   .s7_content {
     color: #7c7a7a;
     font-size: 12px;
   }
   .s7_content text {
     color: #888;
     font-size: 14px;
   }
     .s7_content image {
       margin-left: 10rpx;
     }
     .s7_left {
       margin-top: 20rpx;
       margin-right: 50rpx
     }
     .s7_right {
       margin-left: 20rpx;
       margin-top: 20rpx;
       font-size: 14px;
     }
